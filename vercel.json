{
  "cleanUrls": true,
  "redirects": [
    {"source": "/index.html", "destination": "/", "permanent": true},
    {"source": "/:path.html", "destination": "/:path", "permanent": true}
  ],
  "rewrites": [
    {"source": "/author/:path*", "destination": "/api/gone"},
    {"source": "/category/:path*", "destination": "/api/gone"}
  ]
}
